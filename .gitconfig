[user]
	name = David Li
	email = davidli@cs.stanford.edu
[color]
	ui = true
[diff]
	wsErrorHighlight = all
[format]
	pretty = oneline
[push]
	default = simple
[pull]
	rebase = true
[alias]
	a = commit -a --amend
	c = commit -m
	co = checkout
	cp = cherry-pick
	dc = diff --cached
	l = log --oneline --graph
	p = pull
	r = rebase -i HEAD~20
	rc = rebase --continue
	rh = reset --hard HEAD
	st = status
	su = submodule update
	b = branch
[core]
	editor = nvim
